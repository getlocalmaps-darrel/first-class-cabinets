const serviceSchema = {
  "@context": "https://schema.org",
  "@type": "HomeAndConstructionBusiness",
  "name": "First Class WoodWorks — Custom Cabinets " + city,
  "telephone": "+19519731265",
  "url": "https://www.firstclasswoodworks.com/cities/fallbrook",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "24491 Avenida Arconte",
    "addressLocality": "Murrieta",
    "addressRegion": "CA",
    "postalCode": "92562",
    "addressCountry": "US"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": 33.3764,
    "longitude": -117.2511
  },
  "areaServed": ["Fallbrook", "De Luz", "Live Oak Park", "Morro Hills", "Bonsall"],
  "knowsAbout": ["Finish Carpentry", "Custom Kitchen Islands", "Moisture-Resistant Cabinetry", "3D Design Renderings", "Rustic Hardwood Cabinetry", "Blum Soft-Close Hardware"],
  "priceRange": "$$$",
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "5.0",
    "reviewCount": "25"
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Custom Cabinetry & Remodeling Services in " + city,
    "itemListElement": [
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Custom Kitchen Cabinets", "url": "https://www.firstclasswoodworks.com/services/custom-kitchen-cabinets" } },
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Kitchen Remodeling", "url": "https://www.firstclasswoodworks.com/services/kitchen-remodeling" } },
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Bathroom Remodeling & Custom Vanities", "url": "https://www.firstclasswoodworks.com/services/bathroom-remodeling" } },
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Cabinet Maker", "url": "https://www.firstclasswoodworks.com/services/cabinet-maker" } },
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Custom Woodworking & Finish Carpentry", "url": "https://www.firstclasswoodworks.com/services/custom-woodworking" } },
      { "@type": "Offer", "itemOffered": { "@type": "Service", "name": "Granite Countertop Installation", "url": "https://www.firstclasswoodworks.com/services/granite-countertops" } }
    ]
  }
};

const faqs = [
  {
    question: "How much do custom kitchen cabinets cost in Fallbrook?",
    answer: "Custom cabinets in Fallbrook typically cost $500 to $1,200 per linear foot depending on wood species, finish complexity, and design details. Custom vanities range from $400 to $3,000, and built-ins start around $1,200. This investment delivers furniture-grade plywood construction with Blum lifetime-warranty hardware, built to complement the rustic elegance of Fallbrook's ranch and estate properties."
  },
  {
    question: "Who is the best cabinet maker near Fallbrook, CA?",
    answer: "First Class WoodWorks is the highest-rated custom cabinet maker serving Fallbrook with an A+ BBB rating and 5.0-star reviews. We build every cabinet in-house at our Murrieta shop and install with our own licensed, bonded, and insured finish carpentry team — never subcontractors."
  },
  {
    question: "What Fallbrook areas do you serve for custom cabinets?",
    answer: "We serve the entire Fallbrook community including De Luz, Live Oak Park, Morro Hills, and the neighboring Bonsall area. Every property in the 92028 zip code receives our full design, fabrication, and installation service from our nearby Murrieta workshop."
  },
  {
    question: "How long does a custom cabinet project take in Fallbrook?",
    answer: "Custom cabinet projects in Fallbrook take 8 to 12 weeks from design approval through professional installation. This timeline ensures proper 3D rendering review, premium material procurement, precision fabrication, and a complete conversion varnish cure for maximum longevity."
  },
  {
    question: "Do you build rustic-style cabinets for Fallbrook ranch homes?",
    answer: "Absolutely. We specialize in knotty Alder, reclaimed-look White Oak, and hand-distressed finishes that complement Fallbrook's rural character. Every rustic cabinet still features furniture-grade plywood construction and Blum soft-close hardware for modern performance behind the artisan aesthetic."
  },
  {
    question: "What wood species match the Fallbrook ranch aesthetic?",
    answer: "For Fallbrook estates, knotty Alder delivers warm, natural character while White Oak provides elegant grain patterns with rustic appeal. We also work with Hickory and Cherry for homeowners seeking a more dramatic look. All species are paired with furniture-grade plywood box construction."
  },
  {
    question: "Are custom cabinets a good investment for Fallbrook properties?",
    answer: "Yes. Fallbrook's ranch and estate properties command premium prices, and buyers actively seek custom cabinetry. A kitchen upgrade with heirloom-quality cabinets in the 92028 zip code typically returns 60 to 80 percent on investment while dramatically elevating daily living quality."
  },
  {
    question: "Can you build custom bathroom vanities in Fallbrook?",
    answer: "We specialize in custom vanities, floating units, and linen towers for Fallbrook bathrooms. Each piece is sealed with post-catalyzed conversion varnish for moisture resistance, and we offer rustic finishes that match the character of your Fallbrook ranch or estate home."
  },
  {
    question: "Is First Class WoodWorks licensed to work in Fallbrook?",
    answer: "Yes. We are fully licensed, bonded, and insured for finish carpentry in both Riverside and San Diego Counties, covering all Fallbrook and Bonsall neighborhoods. Our A+ BBB accreditation ensures every project meets California Residential Building Code standards."
  },
  {
    question: "Do you offer free consultations for Fallbrook homeowners?",
    answer: "We provide complimentary in-home consultations throughout Fallbrook and the surrounding area. We take laser measurements, discuss your design vision, and create detailed 3D renderings for your approval before fabrication begins. Call 951-973-1265 to schedule your visit."
  },
  {
    question: "What hardware do you install on Fallbrook custom cabinets?",
    answer: "Every Fallbrook project includes Blum TANDEM undermount slides and CLIP top soft-close hinges with a lifetime warranty. Even our most rustic designs feature this Austrian-engineered hardware for whisper-quiet operation and over 200,000 open-close cycles of reliable performance."
  },
  {
    question: "Can you build custom entertainment centers for Fallbrook homes?",
    answer: "Yes. We hand-craft entertainment centers, fireplace surrounds, and media walls that complement Fallbrook's ranch and estate aesthetics. Each unit conceals wiring, accommodates your specific dimensions, and is built from the same furniture-grade materials as our kitchen cabinetry."
  },
  {
    question: "Do you serve both Riverside and San Diego County sides of Fallbrook?",
    answer: "Yes. While our shop is in Murrieta, we are licensed to work in both Riverside County and San Diego County, covering the full Fallbrook area, Bonsall, and De Luz. Every homeowner in the 92028 zip code is within our standard service radius."
  }
];

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": faqs.map(faq => ({
    "@type": "Question",
    "name": faq.question,
    "acceptedAnswer": {
      "@type": "Answer",
      "text": faq.answer
    }
  }))
};
---

<MainLayout
  title={"Custom Kitchen Cabinets " + city + " | Master Cabinet Maker | " + phone}
  description={"Fallbrook custom cabinet maker. Handcrafted rustic & luxury kitchen cabinets, bathroom vanities & ranch-style built-ins by First Class WoodWorks. A+ BBB. Call " + phone}
>
  <script type="application/ld+json" set:html={JSON.stringify(serviceSchema)} />
  <script type="application/ld+json" set:html={JSON.stringify(faqSchema)} />

  <article class="pb-20 bg-white">
    <div class="max-w-6xl mx-auto px-6">

      <header class="max-w-4xl mb-16 reveal">
        <h1 class="text-5xl md:text-7xl font-serif font-bold text-zinc-900 mb-6 leading-tight">
          Custom Kitchen Cabinets {city} <span class="text-yellow-600">{phone}</span>
        </h1>
        <h2 class="text-2xl font-body font-bold text-zinc-700 mb-8">How much do custom kitchen cabinets cost in Fallbrook?</h2>
        <p class="text-xl text-zinc-600 max-w-2xl leading-relaxed">
          Fallbrook's ranch estates and avocado-grove properties deserve cabinetry as distinctive as the land they sit on. We build rustic and refined — always furniture-grade.
        </p>
      </header>

      <div class="grid grid-cols-1 lg:grid-cols-12 gap-16 mb-24">
        <div class="lg:col-span-8 space-y-8 text-lg text-zinc-600 leading-relaxed">

          <section class="reveal">
            <h2 class="text-3xl font-serif font-bold text-zinc-900 mb-4">Fallbrook's Artisan Cabinet Maker</h2>
            <p>
              Known as the "Friendly Village," <strong>Fallbrook</strong> is a community of sprawling avocado groves, equestrian estates, and custom ranch homes nestled between <strong>De Luz</strong>, <strong>Live Oak Park</strong>, and <strong>Morro Hills</strong>. The character of this <strong>92028</strong> zip code demands cabinetry with personality — not the generic white-box look of mass-produced retail units. <a href="https://www.firstclasswoodworks.com/" class="text-yellow-600 font-bold hover:underline">First Class WoodWorks</a> is the <strong>cabinet maker near Fallbrook</strong> that crafts bespoke cabinetry matching the rustic elegance of your property.
            </p>
            <p>
              We are licensed, bonded, and insured for finish carpentry in both Riverside and San Diego Counties — covering every Fallbrook and <strong>Bonsall</strong> address. Our A+ BBB-rated team builds every cabinet in-house using furniture-grade plywood with dovetail drawer boxes, mortise-and-tenon face frames, and <strong>Blum soft-close hardware</strong> backed by a lifetime warranty. No subcontractors. No particle board. No compromises.
            </p>
          </section>

          <section class="reveal">
            <h2 class="text-3xl font-serif font-bold text-zinc-900 mb-4">Rustic Character Meets Modern Precision</h2>
            <p>
              Fallbrook homeowners often seek a look that honors the area's agricultural roots — warm knotty Alder, hand-distressed White Oak, or richly grained Hickory. Our <a href="/services/custom-kitchen-cabinets" class="text-yellow-600 font-bold hover:underline">custom kitchen cabinets</a> deliver that artisan aesthetic without sacrificing modern function. Behind every rustic door panel sits furniture-grade plywood construction, precision-fit to the 1/16th of an inch, with Blum undermount slides rated for over 200,000 cycles.
            </p>
            <p>
              Every Fallbrook project starts with a complimentary in-home consultation. We study your home's architecture, take laser measurements, and produce <strong>3D design renderings</strong> that capture the exact wood species, finish tone, and hardware you envision. Whether you need a full <a href="/services/kitchen-remodeling" class="text-yellow-600 hover:underline">kitchen remodel</a> for an estate on <strong>De Luz Road</strong> or a cabinet refresh for a ranch home near the <strong>Grand Tradition Estate</strong>, our <strong>master cabinet builder</strong> team delivers.
            </p>
          </section>

          <div class="bg-zinc-50 border-l-4 border-yellow-600 p-8 my-10 reveal">
            <h4 class="text-zinc-900 font-bold mb-2">Master Cabinet Maker Pro-Tip:</h4>
            <p class="italic">"For Fallbrook ranch kitchens, we use knotty Alder with a hand-rubbed, glazed finish that accentuates the natural knots and grain — giving every cabinet door a one-of-a-kind character. Unlike stain-grade paint, our multi-step glaze process highlights wood texture rather than hiding it, creating the artisan warmth that defines Fallbrook living."</p>
          </div>

          <section class="reveal">
            <h2 class="text-3xl font-serif font-bold text-zinc-900 mb-4">Complete Cabinetry for Fallbrook Estates</h2>
            <p>
              Our craftsmanship extends well beyond the kitchen. Fallbrook homeowners trust us for <a href="/services/bathroom-remodeling" class="text-yellow-600 hover:underline">rustic-finished bathroom vanities</a> sealed against daily moisture, <a href="/services/custom-closets" class="text-yellow-600 hover:underline">walk-in closet systems</a> built from solid hardwoods, and <a href="/services/media-centers" class="text-yellow-600 hover:underline">hand-crafted entertainment centers</a> that complement your ranch-style living room. Every piece is finished with post-catalyzed conversion varnish for lifetime protection.
            </p>
            <p>
              From the hilltop estates of <strong>Morro Hills</strong> to the grove properties of <strong>Live Oak Park</strong>, we build cabinetry that matches the distinctive character of Fallbrook living. See the projects we are delivering to homeowners in <a href="/cities/canyon-lake" class="text-yellow-600 hover:underline">Canyon Lake</a>, or call <strong>{phoneFull}</strong> to start your free in-home consultation. Your Fallbrook kitchen deserves a true artisan.
            </p>
          </section>

        </div>

        <aside class="lg:col-span-4 space-y-8 sticky top-32">
          <div class="bg-zinc-900 text-white p-10 rounded-sm reveal">
            <h3 class="font-serif font-bold text-2xl mb-4 text-yellow-600">Fallbrook's Cabinet Experts</h3>
            <ul class="space-y-3 text-white/80 text-sm mb-8">
              <li class="border-b border-white/10 pb-2">Licensed in Riverside & San Diego Co.</li>
              <li class="border-b border-white/10 pb-2">A+ BBB Accredited</li>
              <li class="border-b border-white/10 pb-2">Rustic & Custom Finishes</li>
              <li class="border-b border-white/10 pb-2">Blum Lifetime-Warranty Hardware</li>
              <li class="border-b border-white/10 pb-2">Free 3D Design Renderings</li>
              <li>Serving Fallbrook 92028</li>
            </ul>
            <a href="tel:9519731265" class="block text-center border border-yellow-600 py-4 font-bold hover:bg-yellow-600 transition tracking-wider">
              {phoneFull}
            </a>
            <a href="/contact" class="mt-4 block text-center bg-yellow-600 text-zinc-900 py-4 font-bold uppercase tracking-widest text-xs hover:bg-yellow-700 transition">
              Get My Free Quote
            </a>
          </div>
          <div class="bg-zinc-50 border border-zinc-200 p-6 rounded-sm reveal">
            <h3 class="font-serif font-bold text-lg mb-4 text-zinc-900">Our Service Area</h3>
            <ul class="grid grid-cols-2 gap-x-4 gap-y-2 text-sm">
              <li><a href="/cities/murrieta" class="text-yellow-600 hover:underline">Murrieta</a></li>
              <li><a href="/cities/temecula" class="text-yellow-600 hover:underline">Temecula</a></li>
              <li><a href="/cities/menifee" class="text-yellow-600 hover:underline">Menifee</a></li>
              <li><a href="/cities/winchester" class="text-yellow-600 hover:underline">Winchester</a></li>
              <li><a href="/cities/french-valley" class="text-yellow-600 hover:underline">French Valley</a></li>
              <li><a href="/cities/wildomar" class="text-yellow-600 hover:underline">Wildomar</a></li>
              <li><a href="/cities/canyon-lake" class="text-yellow-600 hover:underline">Canyon Lake</a></li>
              <li><a href="/cities/lake-elsinore" class="text-yellow-600 hover:underline">Lake Elsinore</a></li>
              <li><a href="/cities/perris" class="text-yellow-600 hover:underline">Perris</a></li>
              <li><span class="text-zinc-900 font-bold">Fallbrook</span></li>
            </ul>
          </div>
        </aside>
      </div>

      <!-- FAQ Section -->
      <section class="py-16 border-t border-zinc-200">
        <div class="max-w-4xl mx-auto">
          <div class="text-center mb-16 reveal">
            <h2 class="text-3xl md:text-5xl font-serif font-bold text-zinc-900 mb-6">
              Custom Cabinet <span class="text-yellow-600">FAQs</span> — Fallbrook
            </h2>
            <div class="h-1 w-20 bg-yellow-600 mx-auto rounded-full mb-6"></div>
            <p class="text-zinc-500 italic">Common questions about custom cabinetry, costs, and timelines for Fallbrook homeowners.</p>
          </div>
          <div class="space-y-4">
            {faqs.map((faq, index) => (
              <details class="group bg-white border border-zinc-200 rounded-sm shadow-sm hover:shadow-md transition-all duration-300 reveal" style={`transition-delay: ${index * 80}ms`}>
                <summary class="flex justify-between items-center cursor-pointer p-6 list-none">
                  <h3 class="text-lg md:text-xl font-serif font-bold text-zinc-800 group-hover:text-yellow-600 transition-colors pr-8">
                    {faq.question}
                  </h3>
                  <span class="text-yellow-600 transition-transform duration-300 group-open:rotate-180 shrink-0">
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" class="w-6 h-6">
                      <path stroke-linecap="round" stroke-linejoin="round" d="M19.5 8.25l-7.5 7.5-7.5-7.5" />
                    </svg>
                  </span>
                </summary>
                <div class="px-6 pb-6 text-zinc-600 leading-relaxed border-t border-zinc-100 pt-4">
                  {faq.answer}
                </div>
              </details>
            ))}
          </div>
        </div>
      </section>

    </div>
  </article>
</MainLayout>

<style>
  details > summary { list-style: none; }
  details > summary::-webkit-details-marker { display: none; }
</style>
